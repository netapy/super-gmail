{
    "manifest_version": 3,
    "name": "Super Gmail",
    "version": "1.0",
    "description": "Open source Gmail AI extension",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://mail.google.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}